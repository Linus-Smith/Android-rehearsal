<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yang.mdevelopers.MainActivity">

    <Button
        android:id="@+id/bt_canvas_rotate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="canvas_rotate"/>
    <Button
        android:id="@+id/bt_provider"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="content Provider"/>
    <Button
        android:id="@+id/bt_thread_one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="生产者消费者"/>

    <Button
        android:id="@+id/bt_jni_dome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="JNI DOME"/>

    <Button
        android:id="@+id/bt_scale_gesture"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="手势缩放"/>

</LinearLayout>
